<launch>
	<node  name="rslidar16_right_driver" pkg="rslidar_driver" exec="rslidar_node" >
		<remap from="rslidar_packets" to="rslidar16_right_packets"/>
		<remap from="rslidar_packets_difop" to="rslidar16_right_packets_difop"/>
		<param name="frame_id" value="rslidar16_right"/>
		<param name="model" value="RS16"/>
		<param name="device_ip" value="192.168.2.200" />
		<param name="host_ip" value="192.168.2.102" />
		<param name="msop_port" value="6699" />
		<param name="difop_port" value="7788"/>
		<param name="cut_angle" value="0.0" />
		<!--param name="pcap" value="path_to_pcap"/-->
	</node>

	<node  name="rslidar16_right_pointcloud" pkg="rslidar_pointcloud_stamped" exec="cloud_node_stamped" >
		<remap from="rslidar_packets" to="rslidar16_right_packets"/>
		<remap from="rslidar_packets_difop" to="rslidar16_right_packets_difop"/>
		<param name="output_points_topic" value="rslidar16_right_points" />
		<param name="model" value="RS16"/>
		<param name="curves_path" value="$(find-pkg-share rslidar_pointcloud_stamped)/data/configuration_files_right_rear_rs16_161192000953/curves.csv" />
		<param name="angle_path" value="$(find-pkg-share rslidar_pointcloud_stamped)/data/configuration_files_right_rear_rs16_161192000953/angle.csv" />
		<param name="channel_path" value="$(find-pkg-share rslidar_pointcloud_stamped)/data/configuration_files_right_rear_rs16_161192000953/ChannelNum.csv" />
		<param name="max_distance" value="200.0"/>
		<param name="min_distance" value="0.2"/>
		<param name="resolution_type" value="0.5cm"/>
		<param name="intensity_mode" value="1"/>
	</node>
</launch>
